<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#11ffffff" />
            <corners android:radius="@dimen/inactivePlayerTextSideMargin" />
        </shape>
    </item>
    <item android:bottom="@dimen/smallSizedStroke">
        <shape android:shape="rectangle">
            <solid android:color="@color/newDialogActiveAreaBg" />
            <stroke
                android:width="@dimen/smallSizedStroke"
                android:color="#66000000" />
            <corners android:radius="@dimen/inactivePlayerTextSideMargin" />

        </shape>
    </item>
</layer-list>
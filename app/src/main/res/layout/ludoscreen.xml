<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <RelativeLayout
        android:id="@+id/rl"
        android:layout_width="@dimen/widthsquare"
        android:layout_height="@dimen/widthsquare"
        android:gravity="center">

        <RelativeLayout
            android:id="@+id/layoutf"
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/widthsignlesquare148"
            android:background="@color/colorYellow"></RelativeLayout>

        <include
            layout="@layout/rowleftt"
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/width74"
            android:layout_alignParentStart="true"
            android:layout_centerVertical="true"
            android:background="@color/colorAccent"></include>

        <include
            layout="@layout/rowright"
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/width74"
            android:layout_alignParentEnd="true"
            android:layout_centerVertical="true"
            android:background="@color/colorAccent"></include>

        <RelativeLayout
            android:layout_width="@dimen/width74"
            android:layout_height="@dimen/widthsignlesquare148"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:background="@color/colorAccent"></RelativeLayout>

        <RelativeLayout
            android:layout_width="72dp"
            android:layout_height="71dp"
            android:layout_centerInParent="true"
            android:background="@color/colorPrimaryDark"></RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/widthsignlesquare148"
            android:layout_alignParentEnd="true"
            android:layout_alignParentTop="true"
            android:background="@color/colorBlue">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/widthsignlesquare148"
            android:layout_alignParentBottom="true"
            android:layout_alignParentStart="true"
            android:background="@color/colorGreen">

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="@dimen/widthsignlesquare148"
            android:layout_height="@dimen/widthsignlesquare148"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true"
            android:background="@color/colorRed">

        </RelativeLayout>

    </RelativeLayout>

</FrameLayout>
